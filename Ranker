//function vote(memberId) {
  const member = document.getElementById(memberId);
  let votes = parseInt(member.dataset.votes) || 0;
  votes++;
  member.dataset.votes = votes;
  member.querySelector('.votes').textContent = `Votes: ${votes}`;
}
